
func Min[a: i32, b: i32] => [result: i32]
    where result <= a && result <= b
{
    if (a < b) {
        return a
    } else {
        return b
    }
}

func Max[a: i32, b: i32] => [result: i32]
    where result >= a && result >= b
{
    if (a > b) {
        return a
    } else {
        return b
    }
}

func Abs[a: i32] => [result: i32] 
    where result >= 0
{
    if (a > 0) {
        return a;
    } else {
        return -a;
    }
}